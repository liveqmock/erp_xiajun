<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.wangqin.globalshop.biz1.app.dal.mapperExt.ShippingTrackDOMapperExt" >


    <select id="selectShippingTrackListByLogisticNo" resultMap="com.wangqin.globalshop.biz1.app.dal.mapper.ShippingTrackDOMapper.BaseResultMap" parameterType="java.lang.String" >
        select
        <include refid="com.wangqin.globalshop.biz1.app.dal.mapper.ShippingTrackDOMapper.Base_Column_List" />
        from shipping_track
        where logistic_no = #{logisticNo}
        order by gmt_create desc;
    </select>

    <select id="queryShippingTrack" resultMap="com.wangqin.globalshop.biz1.app.dal.mapper.ShippingTrackDOMapper.BaseResultMap" parameterType="java.lang.String" >
        select
        <include refid="com.wangqin.globalshop.biz1.app.dal.mapper.ShippingTrackDOMapper.Base_Column_List" />
        from shipping_track
        where logistic_no = #{logisticNo}
        order by gmt_create desc;
    </select>

</mapper>
