  <select id="queryResourcesCount" resultType="java.lang.Integer"  parameterType="com.wangqin.globalshop.biz1.app.vo.ResourceQueryVO">
    SELECT
    count(1)
    FROM
    `resource` T1
    where
    1=1
    <if test=" name != null and name != '' ">
      and T1.name = #{name}
    </if>
    <if test=" pid != null ">
      and T1.pid = #{pid}
    </if>
    <if test=" resourceType != null ">
      and T1.resource_type = #{resourceType}
    </if>
    <if test=" startGmt != null ">
      and   <![CDATA[ T1.gmt_create >= #{startGmt} ]]>
    </if>
    <if test=" endGmt != null ">
      and    <![CDATA[ T1.gmt_create <= #{endGmt} ]]>
