<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.wangqin.globalshop.channel.dal.mapperExt.ChannelAccountDOMapperExt" >

    <sql id="tableSection">channel_account</sql>




    <sql id="commonSelectSection">
        1 = 1
        <if test="@com.wangqin.globalshop.common.utils.StringUtils@isNotBlank(channelNo)">
            and T.channel_no = #{channelNo}
        </if>
        <if test="@com.wangqin.globalshop.common.utils.StringUtils@isNotBlank(channelName)">
            and T.channel_name = #{channelName}
        </if>
        <if test="@com.wangqin.globalshop.common.utils.StringUtils@isNotBlank(companyNo)">
            and T.company_no = #{companyNo}
        </if>
        <if test="@com.wangqin.globalshop.common.utils.StringUtils@isNotBlank(type)">
            and T.type = #{type}
        </if>
        <if test="@com.wangqin.globalshop.common.utils.StringUtils@isNotBlank(shopCode)">
            and T.shop_code = #{shopCode}
        </if>
        <if test="@com.wangqin.globalshop.common.utils.StringUtils@isNotBlank(status)">
            and T.status = #{status}
        </if>
        <if test="@com.wangqin.globalshop.common.utils.StringUtils@isNotBlank(appKey)">
            and T.app_key = #{appKey}
        </if>
        <if test="@com.wangqin.globalshop.common.utils.StringUtils@isNotBlank(accessKey)">
            and T.access_key = #{accessKey}
        </if>
        <if test=" isDel == true">
            and T.is_del is true
        </if>
        <if test=" isDel == false">
            and T.is_del is false
        </if>

    </sql>


    <!--查询总条数 -->
    <select id="queryPo" resultMap="com.wangqin.globalshop.biz1.app.dal.mapper.ChannelAccountDOMapper.BaseResultMap" parameterType="com.wangqin.globalshop.biz1.app.dal.dataSo.ChannelAccountSo">
        SELECT <include refid="com.wangqin.globalshop.biz1.app.dal.mapper.ChannelAccountDOMapper.Base_Column_List" />
        FROM channel_account T
        WHERE
        <include refid="commonSelectSection"/>
    </select>

    <!--查询总条数 -->
    <select id="queryPoCount" resultType="java.lang.Integer" parameterType="com.wangqin.globalshop.biz1.app.dal.dataSo.ChannelAccountSo">
        SELECT COUNT(*)
        FROM channel_account T
        WHERE
       <include refid="commonSelectSection"/>
    </select>

    <!-- -->
    <select id="queryPoList" resultMap="com.wangqin.globalshop.biz1.app.dal.mapper.ChannelAccountDOMapper.BaseResultMap">
        SELECT  <include refid="com.wangqin.globalshop.biz1.app.dal.mapper.ChannelAccountDOMapper.Base_Column_List" />
        FROM channel_account T
        WHERE
        <include refid="commonSelectSection"/>
        ORDER BY T.id desc
    </select>

</mapper>
