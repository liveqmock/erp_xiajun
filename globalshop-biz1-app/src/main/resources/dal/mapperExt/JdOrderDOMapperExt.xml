<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.wangqin.globalshop.biz1.app.dal.mapperExt.JdOrderDOMapperExt" >

    <sql id="tableSection">jd_order</sql>

    <resultMap id="BaseResultMapExt" type="com.wangqin.globalshop.biz1.app.dal.dataObject.JdOrderDO" extends="com.wangqin.globalshop.biz1.app.dal.mapper.JdOrderDOMapper.BaseResultMap">
    </resultMap>




    <sql id="commonSelectSection">
        1 = 1
        <if test="channelNo != null">
            and T.channel_no = #{channelNo}
        </if>



        <if test="shopCode != null">
            and T.shop_code = #{shopCode}
        </if>

        <if test="sendStatus != null">
            and T.send_status = #{sendStatus}
        </if>
        <if test="channelOrderNo != null">
            and T.channel_order_no = #{channelOrderNo}
        </if>



        <if test=" isDel == true">
            and T.is_del is true
        </if>
        <if test=" isDel == false">
            and T.is_del is false
        </if>
        <if test=" isDel == null">
            and T.is_del is false
        </if>

    </sql>


    <!--查询总条数 -->
    <select id="searchJdOrder" resultMap="BaseResultMapExt"
    >
        SELECT <include refid="com.wangqin.globalshop.biz1.app.dal.mapper.JdOrderDOMapper.Base_Column_List" />
        FROM <include refid="tableSection"/> T
        WHERE
        <include refid="commonSelectSection"/>
    </select>

    <!--查询总条数 -->
    <select id="searchJdOrderCount" resultType="java.lang.Long"
    >
        SELECT COUNT(*)
        FROM <include refid="tableSection"/> T
        WHERE
        <include refid="commonSelectSection"/>
    </select>

    <!-- -->
    <select id="searchJdOrderList" resultMap="BaseResultMapExt">
        SELECT  <include refid="com.wangqin.globalshop.biz1.app.dal.mapper.JdOrderDOMapper.Base_Column_List" />
        FROM <include refid="tableSection"/> T
        WHERE
        <include refid="commonSelectSection"/>
        ORDER BY T.id desc
    </select>

</mapper>
