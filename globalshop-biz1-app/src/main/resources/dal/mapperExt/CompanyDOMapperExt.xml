<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.wangqin.globalshop.biz1.app.dal.mapperExt.CompanyDOMapperExt" >




    <sql id="commonSelectSection">
        1 = 1
        <if test="companyNo != null">
            and T.company_no = #{companyNo}
        </if>
        <if test="companyName != null">
            and T.company_name = #{companyName}
        </if>

        <if test="status != null">
            and T.status = #{status}
        </if>
        <if test="shopName != null">
            and T.shop_name = #{shopName}
        </if>
        <if test="forceIdcard != null">
            and T.force_idcard = #{forceIdcard}
        </if>
        <if test=" isDel == true">
            and T.is_del is true
        </if>
        <if test=" isDel == false">
            and T.is_del is false
        </if>

    </sql>



    <select id="queryPoList" resultMap="com.wangqin.globalshop.biz1.app.dal.mapper.CompanyDOMapper.BaseResultMap" parameterType="com.wangqin.globalshop.biz1.app.dal.dataObject.CompanyDO">
        SELECT  <include refid="com.wangqin.globalshop.biz1.app.dal.mapper.CompanyDOMapper.Base_Column_List" />
        FROM company T
        WHERE
        <include refid="commonSelectSection"/>
        ORDER BY T.id desc
    </select>



</mapper>
