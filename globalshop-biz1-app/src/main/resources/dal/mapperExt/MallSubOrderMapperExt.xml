        where

        is_del is false
        and status = #{status}
        and <![CDATA[ gmt_create < date_sub(now(), interval #{timeOut} MINUTE ) ]]>

    </select>

    <update id="updateSubOrderStatus">
        UPDATE mall_sub_order
        SET
        status=#{newStatus}
        <where>
            is_del is false
            and status = #{oldStatus}
