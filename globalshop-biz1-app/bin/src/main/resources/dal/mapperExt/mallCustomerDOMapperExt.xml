<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.wangqin.globalshop.biz1.app.dal.mapperExt.MallCustomerDOMapperExt" >
	<select id="selectByOpenId" resultMap="com.wangqin.globalshop.biz1.app.dal.mapper.MallCustomerDOMapper.BaseResultMap">
    select 
    <include refid="com.wangqin.globalshop.biz1.app.dal.mapper.MallCustomerDOMapper.Base_Column_List" />
    from mall_customer
    where open_id = #{openId}
  </select>
	<select id="selectByRole" resultMap="com.wangqin.globalshop.biz1.app.dal.mapper.MallCustomerDOMapper.BaseResultMap">
    select
    <include refid="com.wangqin.globalshop.biz1.app.dal.mapper.MallCustomerDOMapper.Base_Column_List" />
    from mall_customer
    where role = #{role}
  </select>
</mapper>